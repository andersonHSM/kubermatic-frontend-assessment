{
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"name": "db",
	"projectType": "application",
	"sourceRoot": "libs/db/src",
	"targets": {
		"generate": {
			"executor": "nx:run-commands",
			"options": {
				"command": "npx prisma generate --schema libs/db/prisma/schema.prisma"
			}
		},
		"migrate-dev": {
			"executor": "nx:run-commands",
			"options": {
				"command": "npx prisma migrate dev --schema libs/db/prisma/schema.prisma"
			}
		},
		"migrate-deploy": {
			"executor": "nx:run-commands",
			"options": {
				"command": "npx prisma migrate deploy --schema libs/db/prisma/schema.prisma"
			}
		},
		"migrate-reset": {
			"executor": "nx:run-commands",
			"options": {
				"command": "npx prisma migrate reset --force --skip-generate --schema libs/db/prisma/schema.prisma"
			}
		},
		"studio": {
			"executor": "nx:run-commands",
			"options": {
				"command": "npx prisma studio --schema libs/db/prisma/schema.prisma"
			}
		},
		"seed": {
			"executor": "nx:run-commands",
			"options": {
				"command": "node -r ts-node/register libs/db/src/seed.ts"
			}
		}
	},
	"tags": []
}
